<p id="notice"><%= notice %></p>

<h1>Articles</h1>
<br>
<% @articles.each do |article| %>
  <h3><%= article.title %></h3>
  <%= truncate(article.article, :length => 100,  :separator => ' ') do
    link_to "see more...", article
  end %>
<% end %>

<br>
<%= link_to 'New Article', new_article_path %>
